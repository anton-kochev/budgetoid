{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 80,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "options": {
        "trailingComma": "es5"
      }
    }
  ]
}